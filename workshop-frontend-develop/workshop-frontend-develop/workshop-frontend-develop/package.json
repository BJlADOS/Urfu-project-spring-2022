    "build:prod": "NODE_ENV=production better-npm-run build",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --mode development --hot --progress --color --port 8080",
      "env": {
        "NODE_ENV": "development",
        "ENABLE_FAKES": true,
        "APP_API": "/api/v1",
        "LOGIN_URFU": true
      }
    },
    "build": {
      "command": "webpack -p --progress --colors",
      "env": {
        "ENABLE_FAKES": false,
        "APP_API": "/api/v1",
        "LOGIN_URFU": true
      }
    }
  },
